# Listing Aliases
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'

# Git Aliases
alias gs='git status'
alias gc='git commit -m'
alias gsc='git commit -S -m'
alias gl='git log'
alias g1='git log --oneline'
alias gf='git fetch --all'
alias gp='git pull'
alias ga='git add'
alias gaa='git add -A'
alias gb='git reset HEAD^'
alias gpo='git push'